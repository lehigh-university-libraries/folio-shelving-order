<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>FOLIO Shelving Order</display-name>
  <servlet>
    <servlet-name>Shelving Order Web Service</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jakarta.ws.rs.Application</param-name>
      <param-value>edu.lehigh.libraries.shelving_order.ShelvingOrderWebService</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Shelving Order Web Service</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
